{
  "manifest_version": 2,
  "name": "Bookmark Reader",
  "version": "1.0",
  "description": "Check your bookmarks",
  "permissions": [
    "bookmarks",
    "tabs"
  ],
  "icons": {
    "16": "assets/icon16.png",
    "32": "assets/icon32.png"
  },
  "browser_action": {
    "default_popup": "index.html"
  }
}